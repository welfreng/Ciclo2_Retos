package com.mycompany.holamundo;

/**
 *
 * @author Compumag
 */
 

public class ClasePrincipalReto2 {
    public static void main(String[] args) {
    
        //reproductor
        String[] cancionesEscogidas = new String[]{
            "Mil horas",
            "Por ese hombre",
            "El sereno",
            "A esa mujer",
            "Sublime mujer",
            "Vine a conquistarte",
            "Si me dejas ahora",
            "Como un pintor",
            "Sin medir distancia",
            "Mi hermano y yo",
            "Mami",
            "Solo a ti pertenezco",
            "Quiereme",
            "Sencillamente",
            "Enciendeme",
            "Cuando me enamoro",
            "Tu eres mi tesoro"
        };
        ReproductorMusica reproductor = new ReproductorMusica(cancionesEscogidas);
        
        reproductor.devolverCancion();
        reproductor.devolverCancion();
        
        reproductor.agregarCancionesFavoritas();
        reproductor.proximaCancion();
        
        
        //EPS
        String[] cola = {"A0", "A1", "A2", "A3", "A4", "A5", "A6", "A7", "A8", "A9", "A10", "A11"};
        TurnoVirtual turnoVirtual1 = new TurnoVirtual(cola);
        
        turnoVirtual1.atenderProximoTurno();
        turnoVirtual1.atenderProximoTurno();
        turnoVirtual1.atenderProximoTurno();
        turnoVirtual1.atenderProximoTurno();
    }
    
}
